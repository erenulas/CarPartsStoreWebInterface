<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>CarPartsStore</display-name>

 
 <servlet>
 	<servlet-name>Login</servlet-name>
 	<servlet-class>carpartsstore.servlet.LoginServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>Login</servlet-name>
 	<url-pattern>/login</url-pattern>
 </servlet-mapping>
 <servlet>
 	<servlet-name>Orders</servlet-name>
 	<servlet-class>carpartsstore.servlet.OrderServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>Orders</servlet-name>
 	<url-pattern>/orders</url-pattern>
 </servlet-mapping>
 <servlet>
 	<servlet-name>OrderContent</servlet-name>
 	<servlet-class>carpartsstore.servlet.OrderContentServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>OrderContent</servlet-name>
 	<url-pattern>/ordercontent</url-pattern>
 </servlet-mapping>
 
 <servlet>
 	<servlet-name>UpdateLastName</servlet-name>
 	<servlet-class>carpartsstore.servlet.updateLastNameServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>UpdateLastName</servlet-name>
 	<url-pattern>/updateLastName</url-pattern>
 </servlet-mapping>
 
  <servlet>
 	<servlet-name>CreateOrder</servlet-name>
 	<servlet-class>carpartsstore.servlet.CreateOrderServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>CreateOrder</servlet-name>
 	<url-pattern>/createOrder</url-pattern>
 </servlet-mapping>
 
 
 <welcome-file-list>
    
   <welcome-file>index.html</welcome-file>
    
 </welcome-file-list>
</web-app>